{
  "manifest_version": 3,
  "name": "Temu Drop",
  "version": "1.0",
  "description": "Getting tracking temu product data",
  "permissions": [
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/logo_16x16.png",
    "32": "images/logo_32x32.png",
    "48": "images/logo_48x48.png",
    "128": "images/logo_128x128.png"
  }
} 